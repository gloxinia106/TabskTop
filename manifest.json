{
  "manifest_version": 2,
  "name": "Tabsktop Option",
  "version": "1.0",
  "description": "Tab desktop extension",

  "browser_action": {
    "default_popup": "option.html",
    "default_icon": {
      "16": "chromeImg/Img_16.png",
      "32": "chromeImg/Img_32.png",
      "48": "chromeImg/Img_48.png",
      "128": "chromeImg/Img_128.png"
    }
  },

  "icons": {
    "16": "chromeImg/Img_16.png",
    "32": "chromeImg/Img_32.png",
    "48": "chromeImg/Img_48.png",
    "128": "chromeImg/Img_128.png"
  },

  "chrome_url_overrides": {
      "newtab": "index.html"
  },

  "permissions": [
    "activeTab",
    "geolocation"
  ]
}